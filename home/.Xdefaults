!! .Xdefaults
!! Mac Radigan
gnuplot*exportselection: off
gnuplot*fastrotate: on
gnuplot*ctrlq: off
*xterm*background: #000000
*xterm*foreground: #ffffff
*xterm*cursorColor: #d0d0d0
!*xterm*font: xft:apl385 unicode:style=regular:pixelsize=14:antialias=false
*urxvt*perl-lib: /usr/lib64/urxvt/perl/
!URxvt.perl-lib-ext-common: default,matcher,-option-popup,-selection-popup,-realline
*urxvt*perl-lib-ext-common: default,matcher,selection
*urxvt*urlLauncher: /usr/bin/google-chrome
*urxvt*underlineUrls: true
*urxvt*matcher.button: 1
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
*urxvt*background: #000000
*urxvt*foreground: #ffffff
*urxvt*cursorColor: #cc1100
!*urxvt*cursorColor: #d0d0d0
!*urxvt*cursorUnderline: true
*urxvt*cursorBlink: true
*urxvt*fading: 30
*urxvt*font: xft:Terminus:pixelsize=18
URxvt.inputMethod: ibus
URxvt.preeditType: OverTheSpot
!! *EOF*
